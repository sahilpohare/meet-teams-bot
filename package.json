{
    "name": "meet-teams-bot",
    "version": "1.0.0",
    "description": "Automated meeting recording bot for Google Meet, Microsoft Teams, and Zoom",
    "keywords": [
        "meeting",
        "recording",
        "bot",
        "automation",
        "google-meet",
        "teams",
        "playwright"
    ],
    "engines": {
        "node": ">= 18.0.0 <=22",
        "bun": ">= 1.0.0"
    },
    "packageManager": "bun@1.1.38",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.873.0",
        "@aws-sdk/lib-storage": "^3.873.0",
        "@azure/arm-appcontainers": "^3.0.0",
        "@azure/functions": "^4.8.0",
        "@azure/identity": "^4.13.0",
        "@azure/storage-blob": "^12.29.1",
        "@hono/node-server": "^1.19.5",
        "@hono/swagger-ui": "^0.5.2",
        "@hono/zod-openapi": "^1.1.4",
        "@playwright/test": "^1.53.0",
        "async": "^3.2.6",
        "axios": "0.21.1",
        "express": "4.17.1",
        "hono": "^4.9.12",
        "retry-axios": "^2.5.0",
        "tslib": "^2.8.1",
        "uuid": "^13.0.0",
        "winston": "^3.17.0",
        "ws": "8.18.0",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@types/async": "^3.2.24",
        "@types/express": "^4.17.11",
        "@types/jest": "^29.5.14",
        "@types/node": "~14.14.45",
        "@types/uuid": "^11.0.0",
        "@types/ws": "8.5.12",
        "depcheck": "^1.4.7",
        "prettier": "3.3.3",
        "rimraf": "~3.0.2",
        "ts-jest": "^29.2.5",
        "ts-node-dev": "^2.0.0",
        "ts-unused-exports": "^11.0.1",
        "typescript": "^5.4",
        "unimported": "^1.31.0"
    },
    "scripts": {
        "start": "bun run build/src/main.js",
        "start:scheduler": "bun run build/src/scheduler-server.js",
        "dev": "bun --watch src/main.ts",
        "dev:scheduler": "bun --watch src/scheduler-server.ts",
        "clean": "rimraf coverage build tmp",
        "build": "echo '{\"buildDate\": \"'$(date -u +'%Y-%m-%dT%H:%M:%SZ')'\"}' > src/buildInfo.json && bun x tsc --skipLibCheck -p tsconfig.release.json",
        "build:scheduler": "bun run build",
        "watch": "bun x tsc --skipLibCheck -w -p tsconfig.release.json",
        "format": "prettier --log-level warn --write \"src/**/*.{jsx,js,ts}\"",
        "test": "bun test",
        "test:watch": "bun test --watch",
        "test:coverage": "bun test --coverage",
        "check:unused-exports": "ts-unused-exports tsconfig.json",
        "check:unused-imports": "unimported",
        "check:unused-deps": "depcheck",
        "check:dead-code": "bun run check:unused-exports && bun run check:unused-imports && bun run check:unused-deps"
    },
    "author": "Meet Teams Bot Contributors",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/meet-teams-bot.git"
    },
    "bugs": {
        "url": "https://github.com/yourusername/meet-teams-bot/issues"
    },
    "homepage": "https://github.com/yourusername/meet-teams-bot#readme",
    "volta": {
        "node": "20.18.0"
    }
}
