{
	"manifest_version": 2,

	"name": "meeting-baas",
	"description": "meeting-baas",
	"version": "1.0",
	"key": "",
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	},

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["./js/vendor.js"]
		}
	],

	"background": {
		"scripts": ["./js/vendor.js", "./js/background.js"],
		"persistent": true
	},

	"permissions": [
		"*://*/*",
		"activeTab",
		"tabs",
		"tabCapture",
		"storage"
	]
}
