{
  "name": "@meet-teams-bot/bot",
  "version": "1.0.0",
  "private": true,
  "description": "Automated meeting recording bot for Google Meet, Microsoft Teams, and Zoom",
  "main": "dist/main.js",
  "scripts": {
    "build": "echo '{\"buildDate\": \"'$(date -u +'%Y-%m-%dT%H:%M:%SZ')'\"}' > src/buildInfo.json && tsc -p tsconfig.json",
    "dev": "bun --watch src/main.ts",
    "start": "node dist/main.js",
    "clean": "rimraf dist",
    "test": "bun test"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.873.0",
    "@aws-sdk/lib-storage": "^3.873.0",
    "@azure/storage-blob": "^12.29.1",
    "@playwright/test": "^1.53.0",
    "async": "^3.2.6",
    "axios": "0.21.1",
    "express": "4.17.1",
    "retry-axios": "^2.5.0",
    "tslib": "^2.8.1",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@types/async": "^3.2.24",
    "@types/express": "^4.17.11",
    "@types/node": "~14.14.45",
    "@types/uuid": "^11.0.0",
    "@types/ws": "8.5.12",
    "typescript": "^5.4"
  },
  "packageManager": "bun@1.3.1"
}
